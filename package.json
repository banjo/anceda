{
    "name": "anceda",
    "type": "module",
    "prettier": "@banjoanton/prettier-config",
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "scripts": {
        "dev": "dotenvx run -- run-p ui:dev server:dev",
        "build": "./scripts/build.sh",
        "preview": "NODE_ENV=production dotenvx run -- node ./build/index.js",
        "playground": "dotenvx run -- tsx src/playground.ts",
        "server:build": "tsup",
        "server:dev": "tsx watch src/index.ts",
        "ui:dev": "vite",
        "ui:build": "vite build",
        "ui:preview": "vite preview",
        "db:generate": "pnpm exec prisma generate",
        "db:migrate:dev": "pnpm exec prisma migrate dev && pnpm run db:generate",
        "db:migrate:prod": "pnpm exec prisma migrate deploy",
        "db:seed": "pnpm exec prisma db seed",
        "db:reset": "pnpm exec prisma migrate reset --force",
        "typecheck": "tsc --noEmit",
        "lint": "eslint .",
        "format": "prettier --write ."
    },
    "dependencies": {
        "@banjoanton/prettier-config": "^0.1.11",
        "@banjoanton/utils": "^0.1.95",
        "@hono/node-server": "^1.13.8",
        "@hono/zod-validator": "^0.4.2",
        "@hookform/resolvers": "^3.10.0",
        "@prisma/client": "5.20.0",
        "@radix-ui/react-dialog": "^1.1.5",
        "@radix-ui/react-dropdown-menu": "^2.1.5",
        "@radix-ui/react-icons": "^1.3.2",
        "@radix-ui/react-label": "^2.1.1",
        "@radix-ui/react-separator": "^1.1.1",
        "@radix-ui/react-slot": "^1.1.1",
        "@radix-ui/react-tabs": "^1.1.2",
        "@radix-ui/react-tooltip": "^1.1.7",
        "@tanstack/react-query": "^5.66.0",
        "@tanstack/react-router": "^1.99.0",
        "bcrypt": "^5.1.1",
        "class-variance-authority": "^0.7.1",
        "close-with-grace": "^2.2.0",
        "clsx": "^2.1.1",
        "cmdk": "^1.0.4",
        "dockerode": "^4.0.4",
        "execa": "^9.5.2",
        "find-up": "^7.0.0",
        "framer-motion": "^11.18.2",
        "fs-extra": "^11.3.0",
        "globby": "^14.0.2",
        "hono": "^4.6.20",
        "lucide-react": "^0.452.0",
        "pino": "^9.6.0",
        "pino-pretty": "^11.3.0",
        "public-ip": "^7.0.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.54.2",
        "react-hot-toast": "^2.5.1",
        "systeminformation": "^5.25.11",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.24.1",
        "zod-validation-error": "^3.4.0"
    },
    "devDependencies": {
        "@banjoanton/eslint-config": "^0.2.17",
        "@dotenvx/dotenvx": "^1.34.0",
        "@tanstack/router-devtools": "^1.99.0",
        "@tanstack/router-plugin": "^1.99.0",
        "@types/bcrypt": "^5.0.2",
        "@types/dockerode": "^3.3.34",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^22.13.0",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.19.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.5.1",
        "prettier": "^3.4.2",
        "prisma": "^5.22.0",
        "tailwindcss": "^3.4.17",
        "tsup": "^8.3.6",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.22.0",
        "vite": "^5.4.14",
        "vite-tsconfig-paths": "^5.1.4"
    }
}
